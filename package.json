{
  "name": "board_the_z",
  "version": "0.1.0",
  "description": "A modern onboarding application built with Next.js, TypeScript, and Supabase featuring glassmorphic UI design",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "mocha -r ts-node/register --extension ts tests/**/*.test.ts",
    "test:api": "mocha -r ts-node/register --extension ts tests/api.test.ts",
    "test:auth": "mocha -r ts-node/register --extension ts tests/auth.test.ts",
    "test:admin": "mocha -r ts-node/register --extension ts tests/admin.test.ts",
    "test:onboarding": "mocha -r ts-node/register --extension ts tests/onboarding.test.ts",
    "test:utils": "mocha -r ts-node/register --extension ts tests/utils.test.ts",
    "test:coverage": "npm run test -- --reporter json > coverage.json",
    "test:watch": "npm run test -- --watch",
    "test:all": "npm run test:utils && npm run test:auth && npm run test:api && npm run test:admin && npm run test:onboarding",
    "github:check": "node scripts/check-github-ready.js",
    "prepare": "npm run build",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "deps:check": "npx depcheck --ignores='@types/*,eslint-*,mocha,chai,ts-node,typescript,@eslint/*'",
    "deps:audit": "npm audit --audit-level=high"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.1",
    "dotenv": "^17.2.1",
    "next": "15.5.0",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "chai": "^6.0.1",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-node": "^30.1.2",
    "jsdom": "^26.1.0",
    "mocha": "^10.0.0",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
